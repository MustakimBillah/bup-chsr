
@model IdentityRole;

@{
    ViewData["Title"] = "DeleteRole";
    ViewData["Name"] = "DeleteRole";
}

<div class="row">
    <div class="col-md-6">
        <h1>Are you sure you want to delete role @Model.Name</h1>
        <form asp-action="DeleteRole" asp-controller="UAM">
            <div class="form-group">
                <button class="btn btn-primary" type="submit">Delete</button>
            </div>
        </form>
    </div>
</div>


