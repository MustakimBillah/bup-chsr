
@{
    Layout = "_ErrorLayout";
}

@model AdmissionApplication

<h1>Admission Form</h1>

<form asp-action="Index" asp-controller="AdmissionApplication">

    @Html.TextBoxFor(x => x.CandidateNameEnglish)
    @Html.ValidationMessageFor(x => x.CandidateNameEnglish, "", new { @class = "text-danger" })

    <button type="submit">Submit</button>
</form>
